    "check": "npm run check:types && npm run check:lint && npm run check:format",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
